{
  "name": "vite-plugin-cjs2esm",
  "version": "1.0.7",
  "description": "",
  "author": "haijie-x",
  "keywords": [
    "vite",
    "commonjs",
    "plugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/haijie-x/vite-plugin-cjs2esm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haijie-x/vite-plugin-cjs2esm.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm run link && pnpm run ts-watch",
    "link": "node ./scripts/link.mjs",
    "ts-watch": "tsup src/index.ts --format esm --watch",
    "lint": "eslint . --fix",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm run lint && pnpm run build",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/node": "^20.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.8.0",
    "execa": "^6.1.0",
    "prettier": "^2.8.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "dependencies": {
    "acorn": "^8.8.2",
    "acorn-walk": "^8.2.0",
    "magic-string": "^0.30.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
